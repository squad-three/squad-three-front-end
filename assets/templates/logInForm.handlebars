<div class='log-in col-xs-offset-2 col-xs-7'>
  <form id='log-in'>
      <input type='email'
        name='email' placeholder='email'>
      <br>
      <input type='password' name='password' placeholder='password'>
      <br>
      <input id='log-in-submit' type='submit' name='submit' value='Log in'>
      <input id='cancel-request' type='button' value='Cancel'>
  </form>
</div>

<div class='register col-xs-3'>
  <input type='button' id='register-request' value='Register'>
  <input type='button' id='log-in-request' value='Log in'>
</div>
